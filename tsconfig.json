{
  "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "jsx": "react-native",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "skipLibCheck": true,
      "noEmit": true,
      "baseUrl": ".",
      "paths": {
          "*": [
              "node_modules/*",
              "types/*"
          ]
      },
      "typeRoots": [
          "node_modules/@types"
      ]
  },
  "include": [
      "node_modules/react-native/generated",
      "src"
  ]
}